#Mappings for Venue.json: 
#<#VenueMapping>

@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix mail: <http://example.com/mail#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix ex: <http://www.example.com/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix transit: <http://vocab.org/transit/terms/> .
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix schema: <http://schema.org/>.
@prefix gn: <http://www.geonames.org/ontology#>.
@prefix geosp: <http://www.telegraphis.net/ontology/geography/geography#> .
@prefix fno: <http://w3id.org/function/ontology#> .


<#VenueMapping>
    rml:logicalSource <#LogicalSource>;
    rr:subjectMap [
     rr:template "http://loc.example.com/city/{city}";
     rr:class schema:City 
    ];


  rr:predicateObjectMap [
      rr:predicate ex:containsS;
      rr:objectMap [
        a <http://semweb.mmlab.be/ns/fnml#FunctionTermMap> ;
        <http://semweb.mmlab.be/ns/fnml#functionValue> <http://ex.com/functionMap/FunMap_1>
      ]
     ].

<#LogicalSource>
     rml:source "src/test/resources/exampleFNO/Venue.json";
     rml:referenceFormulation ql:JSONPath;
     rml:iterator "$" .

<http://ex.com/functionMap/FunMap_1> a [
        rml:logicalSource <#LogicalSource> ;

        rr:subjectMap [
             rr:termType rr:BlankNode;
             rr:class fno:Execution
        ];

        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [
                rr:constant "http://semweb.mmlab.be/ns/grel#contains"
            ]
        ] ;

        rr:predicateObjectMap [
            rr:predicate <http://semweb.mmlab.be/ns/grel#subStringParameter> ;
            rr:objectMap [
                rr:constant "s"
            ]
        ] ;

        rr:predicateObjectMap [
            rr:predicate <http://semweb.mmlab.be/ns/grel#valueParameter> ;
            rr:objectMap [
                rml:reference "city"
            ]
        ]

    ] .

